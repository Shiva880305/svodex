{
  "version": 2,
  "builds": [
    {
      "src": "svodex_web/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/"
    },
    {
      "src": "/(.*)",
      "dest": "svodex_web/wsgi.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10"
  }
}